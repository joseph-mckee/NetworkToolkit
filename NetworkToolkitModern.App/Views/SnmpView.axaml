<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:NetworkToolkitModern.App.ViewModels"
             mc:Ignorable="d" d:DesignWidth="1300" d:DesignHeight="750"
             x:Class="NetworkToolkitModern.App.Views.SnmpView"
             x:DataType="vm:SnmpViewModel">
    <Design.DataContext>
        <vm:SnmpViewModel />
    </Design.DataContext>
    <Grid Margin="10" ColumnDefinitions="Auto,*" RowDefinitions="*,Auto,Auto">
        <Border Grid.RowSpan="2" BoxShadow="0 0 10 0 Indigo" CornerRadius="5" BorderBrush="Indigo"
                BorderThickness="1" Padding="10" />
        <Border Margin="10,0,0,0" Grid.Row="0" BorderBrush="Indigo" BoxShadow="0 0 10 0 Indigo" CornerRadius="5"
                BorderThickness="1"
                Grid.Column="1" />
        <Border HorizontalAlignment="Stretch" Grid.Row="1" Grid.Column="1" BoxShadow="0 0 10 0 Indigo"
                BorderBrush="Indigo" BorderThickness="1"
                CornerRadius="5" Margin="10,10,0,0" Padding="10" />
        <Border Margin="0,10,0,0" BoxShadow="0 0 10 0 Indigo" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2"
                BorderBrush="Indigo" CornerRadius="5" BorderThickness="1" />
    </Grid>
</UserControl>